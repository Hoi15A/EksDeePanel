extends layout

block content
	h2 Statistics
	h3(id='graph') Graph
	p Here's a neat graph for you to enjoy
		for t in test
			br
			=t
	#myDiv
	script.
		var data = [
			{
			x: ['2013-10-04 22:23:00', '2013-11-04 22:23:00', '2013-12-04 22:23:00'],
			y: [1, 3, 6],
			type: 'scatter'
		  }
		];
		Plotly.newPlot('myDiv', data);
	hr
	h3(id='detailed-statistics') Detailed statistics
	i WORK IN PROGRESS
	hr
	h3(id='command-parameters') Command parameters
	i WORK IN PROGRESS
